<%- include('../partials/sign/header') %>
            <div class="col-md-6 position-relative">
                <div class="sign-in-from">
                    <h1 class="mb-0">doctor Sign Up</h1>
                    <form class="mt-4" method="POST" action="/signupdoctor">
                        <div class="form-group">
                            <label for="exampleInputEmail1">Your Full Name</label>
                            <input type="text" name="username" class="form-control mb-0" id="exampleInputEmail1" placeholder="Your Full Name">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail2">Email Address</label>
                            <input type="email" name="email" class="form-control mb-0" id="exampleInputEmail2" placeholder="Email Address">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputPassword1">Password</label>
                            <input type="password" name="password" minlength="7" class="form-control mb-0" id="exampleInputPassword1" placeholder="Password">
                        </div>
                        <% if(error != undefined && error.length > 0){ %>
                            <div class="alert alert-danger text-center">
                                <%= error %>
                            </div>
                        <% } %>
                        
                        <% if(success != undefined && success.length > 0){ %>
                            <div class="alert alert-success">
                                <%= success %>
                            </div>
                        <% } %>
                        <div class="d-inline-block w-100">
                            <div class="custom-control custom-checkbox d-inline-block mt-2 pt-1">
                                <input type="checkbox" required class="custom-control-input" id="customCheck1">
                                <label class="custom-control-label" for="customCheck1">I accept <a href="#" target="_blank">Terms and Conditions</a></label>
                            </div>
                            <button class="btn btn-primary float-right" type="submit">Sign Up</button>
                        </div>
                        <div class="sign-info">
                            <span class="dark-color d-inline-block line-height-2">Already Have Account ? 
                                <a href="/signindoctor">Log In</a>
                            </span>
                            <ul class="iq-social-media">
                                <li><a href="#"><i class="fa fa-home" aria-hidden="true"></i></a></li>
                                <li><a href="/">Home</a></li>
                            </ul>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
        <!-- Signup in END -->
<%- include('../partials/sign/footer') %>
</body>

